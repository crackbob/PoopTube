{
    "manifest_version": 3,
    "name": "PoopTube",
    "version": "0.1",
    "description": "Revert Youtube UI, and edit internal experiments",
    "permissions": [
      "activeTab"
    ],
    "web_accessible_resources": [
      {
        "resources": ["injected.js"],
        "matches": [
            "*://youtube.com/*",
            "*://www.youtube.com/*",
            "*://m.youtube.com/*"
        ]
      }
    ],
    "icons":
    {
        "128": "icon.png",
        "16": "icon.png",
        "48": "icon.png"
    },
    "content_scripts": [
      {
        "run_at": "document_start",
        "matches": [
            "*://youtube.com/*",
            "*://www.youtube.com/*",
            "*://m.youtube.com/*"
        ],
        "js": ["content.js"]
      }
    ]
  }
